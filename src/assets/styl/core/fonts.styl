//  PTSansCaption
//
// ==============================
@font-face
	font-family: 'PTSansCaption'
	src: local('PTSansCaption Regular'),
		url('~@/assets/fonts/pt-sans/PTSansCaption-Regular.woff2') format('woff2'),
		url('~@/assets/fonts/pt-sans/PTSansCaption-Regular.woff') format('woff')
	font-weight: 400
	font-style: normal

@font-face
	font-family: 'PTSansCaption'
	src: local('PTSansCaption Bold'),
		url('~@/assets/fonts/pt-sans/PTSansCaption-Bold.woff2') format('woff2'),
		url('~@/assets/fonts/pt-sans/PTSansCaption-Bold.woff') format('woff')
	font-weight: 700
	font-style: normal
